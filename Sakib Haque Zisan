#include<bits/stdc++.h>
using namespace std;

int main()
{
    for(int i=1; i<=100; i++) 
    {
        if(i%2==0)
            cout << "Sreya: ";
        else
            cout << "Naff: ";


        if(i%6==0 && i%7==0)   
            cout << "Fizzbuzz" << endl;
        else if(i%6==0) 
            cout << "Fizz" << endl;
        else if(i%7==0) 
            cout << "Buzz" << endl;
        else
            cout << i << endl;
    }
    return 0;
}
